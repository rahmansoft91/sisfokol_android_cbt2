<?xml version='1.0' encoding='utf-8'?>
<widget id="sisfokol.cbt.ujian" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CBT</name>
    <description>
        SISFOKOL CBT
    </description>
    <author email="hajirodeon@gmail.com" href="http://github.com/hajirodeon">
        Agus Muhajir
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access origin="http://192.168.0.109/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="./res/icon/android/icon-36x36.png" />
        <icon density="mdpi" src="./res/icon/android/icon-48x48.png" />
        <icon density="xhdpi" src="./res/icon/android/icon-96x96.png" />
        <icon density="xxhdpi" src="./res/icon/android/icon-144x144.png" />
        <icon density="xxxhdpi" src="./res/icon/android/icon-196x196.png" />
        <preference name="android-minSdkVersion" value="22" />
        <preference name="android-targetSdkVersion" value="28" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>


    <plugin name="cordova-plugin-kiosk" spec="https://github.com/hkalina/cordova-plugin-kiosk.git" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-inappbrowser" spec="^2.0.2" />
    <plugin name="cordova-plugin-device" spec="^2.0.1" />
    <plugin name="cordova-plugin-local-notification" spec="~0.9.0-beta.2" />
</widget>
